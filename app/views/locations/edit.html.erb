<h1>Edit your free Shit!</h1>
<%= form_tag "/locations/#{@location.id}", method: :patch do  %>
  <div>
    Name: <input type="text" name="name" value=" <%= @location.name %> ">
  </div>
  <div>
    Description: <input type="text" name="description" value=" <%= @location.description %> ">
  </div>
  <div>
    Address: <input type="text" name="address" value=" <%= @location.address %> ">
  </div>
  <div>
    Start_date: <input type="text" name="start_date" value=" <%= @location.start_date %> ">
  </div>
  <div>
    End_date: <input type="text" name="end_date:" value=" <%= @location.end_date %> ">
  </div>
  <input type="submit" value="Submit">
<% end %>